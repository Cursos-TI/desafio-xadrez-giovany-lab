#include<stdio.h>

int main(){
    //mover a torre 5 casas para a direita
    for(int t = 0;t<5;t++){
        printf("Direita\n");
    }
    int b =0;
    //mover o bispo 5 casas para a direita
    while (b < 5) {
        printf("cima,direita\n", b);
        b++;
    }
    int r = 0;
   //mover a rainha oito casas para a esquerda
   do{
    printf("Esquerda\n");
    r++;
   }while (r <8);
   
   
    return 0;
}
